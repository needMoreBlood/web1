@model System.Collections.Generic.List<WebApplication3.Models.DataAccessPostgreSqlProvider.DbTatooParlor>

@{
    ViewBag.Title = "Записи";
    Layout = "_Layout";
}

<h2>@ViewBag.Title</h2>

@foreach (var ship in Model)
{
<div>
    <table class="table table-bordered table-responsive table-hover">
        <thead>
            <tr>
                <td>Имя</td>
                <td>Возраст</td>
                <td>Контакты</td>
                <td>Пол</td>
                <td>Стиль тату</td>
                <td>Часть тела</td>
                <td>Мастер</td>
                <td>Дата</td>
            </tr>
        </thead>
        @if (ship.Journal != null)
        {
            foreach (var sm in ship.Journal)
            {
        <tr>
            <td>@sm.VisitorName</td>
            <td>@sm.Age</td>
            <td>@sm.Contacts</td>
            <td>@sm.Gender</td>
            <td>@sm.TatooStyles</td>
            <td>@sm.BodyPart</td>
            <td>@sm.Master</td>
            <td>@sm.DateToVisit</td>
        </tr>
            }
        }
    </table>
</div>
}
